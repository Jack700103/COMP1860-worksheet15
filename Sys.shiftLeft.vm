// Sys.shiftLeft function
function Sys.shiftLeft 2
    @SP
    AM=M-1
    D=M
    @y
    M=D

    @SP
    AM=M-1
    D=M
    @x
    M=D


    @x
    D=M
    @result
    M=D

    @0
    D=A
    @counter
    M=D

(SHIFT_LOOP)
    @counter
    D=M
    @y
    D=D-M
    @END_SHIFT
    D;JGE  


    @result
    M=M+M

    @counter
    M=M+1
    @SHIFT_LOOP
    0;JMP

(END_SHIFT)

    @result
    D=M
    @SP
    A=M
    M=D
    @SP
    M=M+1

    @LCL
    D=M
    @R13
    M=D  

    @5
    A=D-A
    D=M
    @R14
    M=D  

    @ARG
    D=M
    @SP
    M=D+1  

    @R13
    D=M
    @1
    D=D-A
    A=D
    D=M
    @THAT
    M=D 

    @R13
    D=M
    @2
    D=D-A
    A=D
    D=M
    @THIS
    M=D  

    @R13
    D=M
    @3
    D=D-A
    A=D
    D=M
    @ARG
    M=D  

    @R13
    D=M
    @4
    D=D-A
    A=D
    D=M
    @LCL
    M=D  

    @R14
    A=M
0;JMP  

function Sys.init 0
    push constant 6
    push constant 2
call Sys.shiftLeft 2
